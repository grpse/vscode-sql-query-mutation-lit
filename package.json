{
  "name": "vscode-sql-lit",
  "version": "0.4.0",
  "engines": {
    "vscode": "^1.22.0"
  },
  "license": "MIT",
  "displayName": "SQL Lit",
  "icon": "images/sql-lit.png",
  "description": "Syntax highlighting for SQL tagged template literals in JavaScript.",
  "publisher": "thebearingedge",
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "keywords": [
    "tagged",
    "template",
    "literal",
    "syntax",
    "highlighting",
    "sql",
    "javascript"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.js",
          "source.js.jsx",
          "source.jsx",
          "source.ts",
          "source.tsx",
          "source.vue",
          "text.html.markdown"
        ],
        "scopeName": "inline.sql-lit",
        "path": "./syntaxes/sql-lit.json",
        "embeddedLanguages": {
          "meta.embedded.block.sql": "sql"
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thebearingedge/vscode-sql-lit"
  }
}
